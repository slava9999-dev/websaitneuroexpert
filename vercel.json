{
  "name": "websaitneuroexpert",
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "build" }
    },
    {
      "src": "frontend/api/**/*.py",
      "use": "@vercel/python"
    },
    {
      "src": "frontend/api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/favicon.ico", "dest": "/favicon.png" },
    { "src": "/api/(.*)", "dest": "/frontend/api/$1" },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
